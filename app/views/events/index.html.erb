<h1>Your Fashion Events!</h1>

<% @events.each_slice(3) do |row| %>
  <div class="row">
    <% row.each do |e| %>
      <div class="col-sm-4">
        <div class="card">
          <%= image_tag(e.url, class: "card-img") %>
          <div class="card-body">
            <h5 class="card-title"><%= link_to(e.url, event_path(g)) %></h5>
          </div>
        </div>
      </div>
    <% end %>
  </div>
<% end %>
